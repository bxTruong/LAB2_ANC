                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".MyReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PHONE_STATE" ></action>
            </intent-filter>
        </receiver>
        <receiver android:name=".MyReceiver2">
            <intent-filter>
                <action android:name="fppoly.android.CUSTOM_INTENT" ></action>
            </intent-filter>
        </receiver>
        <receiver android:name=".MyReceiver3">
            <intent-filter>
                <action android:name="fppoly.android.CUSTOM_INTENT2" ></action>
            </intent-filter>
        </receiver>
    </application>

</manifest>